<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                         https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="App\Context\Account\Domain\Model\Inbox\Inbox" table="_inbox">
        <id name="inboxId" column="inbox__id" type="inbox__id"/>
        <field name="messageName" column="message_name"/>
        <field name="processingTime" column="processing_time" type="float"/>
        <field name="createdAt" column="created_at" type="datetime_immutable"/>
    </entity>
</doctrine-mapping>
