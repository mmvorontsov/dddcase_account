<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                         https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="App\Context\Account\Domain\Model\KeyMaker\RegistrationKeyMaker"
            table="_registration__key_maker">
        <unique-constraints>
            <unique-constraint columns="registration__id"/>
        </unique-constraints>
        <field name="registrationId" column="registration__id" type="registration__id"/>
    </entity>
</doctrine-mapping>
